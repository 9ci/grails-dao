import groovy.transform.*

import gorm.tools.Pager

model {
    List data
    Pager pager
    Map renderArgs = [:]
}

System.out.println "Model in plugins _list $binding.variables"
json {
    page pager.page
    total pager.getPageCount()
    records pager.recordCount
    data g.render(data, renderArgs)
}
